<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <RootSDK>$(MSBuildThisFileDirectory)..\</RootSDK>
	<OutDir>$(RootSDK)output\libs\</OutDir>
    <TargetName>Stormancer_$(Configuration)_$(Platform)</TargetName>
    <LibraryPath>$(RootSDK)binaries\$(Configuration)\$(Platform)\</LibraryPath>
    <IntDir>$(RootSDK)Intermediate\stormancer\$(Platform)_$(Configuration)\</IntDir>
  </PropertyGroup>
  <PropertyGroup />

  <ItemGroup>
    <BuildMacro Include="RootSDK">
      <Value>$(RootSDK)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>